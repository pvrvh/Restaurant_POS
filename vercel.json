@"
{
  "version": 2,
  "builds": [
    {
      "src": "pos-backend/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "pos-frontend/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "pos-frontend/dist" }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "pos-backend/server.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "pos-frontend/dist/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "pos-frontend/dist/index.html"
    }
  ]
}  